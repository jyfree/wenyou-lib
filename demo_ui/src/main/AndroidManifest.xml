<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wenyou.uidemo">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name="debug.DemoUIApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LaunchSimpleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".activity.GuideSimpleActivity" />
        <activity android:name=".activity.SelectorSimpleActivity" />
        <activity android:name=".activity.ShapeTextViewSimpleActivity" />
        <activity android:name=".activity.TouchImageViewSimpleActivity" />
        <activity android:name=".activity.BannerSimpleActivity" />
        <activity android:name=".activity.AlphaViewSimpleActivity" />
        <activity android:name=".activity.CountDownSimpleActivity" />
        <activity android:name=".activity.ShadowSimpleActivity" />
        <activity android:name=".activity.GoodViewSimpleActivity" />
        <activity android:name=".activity.VerifyCodeEditTextSimpleActivity" />
        <activity android:name=".activity.FlowLayoutSimpleActivity" />
        <activity android:name=".activity.TitleBarSimpleActivity" />

    </application>

</manifest>